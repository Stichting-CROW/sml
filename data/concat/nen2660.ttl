nen2660-term:Activity a skos:Concept ;
    skos:broader nen2660-term:Entity ;
    skos:definition "An activity is something possibly or actually happening in space and time"@en ;
    skos:prefLabel "Activity"@en,
        "Aktiviteit"@nl .

