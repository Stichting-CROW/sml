nen2660:MatterPortion
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty nen2660:portion ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass nen2660:Matter ;
      owl:onProperty rdf:value ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
