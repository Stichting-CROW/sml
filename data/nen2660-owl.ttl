<https://w3id.org/nen2660/owl/def>
  a owl:Ontology ;
  owl:imports <http://qudt.org/schema/qudt> ;
  owl:imports <http://qudt.org/vocab/quantitykind> ;
  owl:imports <http://qudt.org/vocab/unit> ;
