.

nen2660:AbstractConcept
  a owl:Class ;
.
nen2660:Activity
  a owl:Class ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom nen2660:Activity ;
      owl:onProperty nen2660:hasPart ;
    ] ;
.
nen2660:AggregationStateType
  a owl:Class ;
  owl:oneOf (
